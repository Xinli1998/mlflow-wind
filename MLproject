name: poly-rnn

conda_env: conda.yaml

entry_points:
  main:
    parameters:
      alpha: {type: float, default: 0.1}
      max_iter: {type: int, default: 10000}
      degree: {type: int, default: 4}
      number_of_splits: {type: int, default: 5}
    command: "python untitled.py {alpha}{max_iter}{degree}{number_of_splits}"
